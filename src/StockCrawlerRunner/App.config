<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="StockCrawler.Dao.Properties.Settings.StockConnectionString"
        connectionString="Data Source=.\SQLEXPRESS;Initial Catalog=Stock;Integrated Security=True"
        providerName="System.Data.OleDb" />
  </connectionStrings>
  <appSettings>
    <add key="DB_TYPE" value="MSSQL"/>
    <add key="DailyCollectorType" value="StockCrawler.Services.StockDailyPrice.TwseStockDailyInfoCollector, StockCrawler.Services"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
