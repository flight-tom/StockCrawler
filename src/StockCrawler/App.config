<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
  </configSections>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <namespace name="StockCrawler.Services.StockBasicInfo" />
    <namespace name="StockCrawler.Services.StockDailyPrice" />
    <namespace name="StockCrawler.Services.StockFinanceReport" />
    <namespace name="StockCrawler.Services.StockSeasonReport" />
    <assembly name="StockCrawler.Services" />

    <container>
      <register type="IStockBasicInfoCollector" mapTo="GoodInfoStockBasicInfoCollector" />
      <register type="IStockDailyInfoCollector" mapTo="TwseStockDailyInfoCollector" />
      <register type="IStockReportCollector" mapTo="TwseReportCollector" />
      <register type="IStockSeasonReportCollector" mapTo="GoodInfoStockSeasonCollector" />
    </container>
  </unity>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
</configuration>
